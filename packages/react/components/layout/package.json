{
  "name": "@toytoy/react-components-layout",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:js && yarn run build:type && yarn run build:css",
