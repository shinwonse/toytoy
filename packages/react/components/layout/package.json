    "@vanilla-extract/css": "^1.14.2",
    "@vanilla-extract/esbuild-plugin": "^2.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.4"
  },
  "peerDependencies": {
    "@toytoy/themes": "workspace:^",
    "react": "*"
  }
}
